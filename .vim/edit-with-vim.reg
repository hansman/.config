Windows Registry Editor Version 5.00

[HKEY_LOCAL_MACHINE\SOFTWARE\Classes\*\shell\editvim]
@="Edit with &vim"
"Icon"="%ProgramFiles%\\Vim\\vim74\\gvim.exe,0"

[HKEY_LOCAL_MACHINE\SOFTWARE\Classes\*\shell\editvim\command]
@="c:\\lib\\msys64\\usr\\bin\\mintty -o AppID=vim -t \"%L\" -e /usr/bin/vim -nc \"cd %%:p:h | map <C-z> <Nop>\" \"%L\""

